import random
a = ["Кушать", "Спать", "Бегать", "Гулять", "Петь", "Танцевать"]
print(random.choice(a))



ОБУЧАЛКА
meme_dict = {
            "КРИНЖ": "Что-то очень странное или стыдное",
            "ЛОЛ": "Что-то очень смешное",
            "ПРУФЫ": "Доказательства"
            }
word = input("Привет, ведите непонятное слово   :   КРИНЖ,ЛОЛ,ПРУФЫ").upper()
if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("я не могу найти слово")
